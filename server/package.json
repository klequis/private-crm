{
  "name": "events-server",
  "version": "0.1.0",
  "description": "server for events project",
  "scripts": {
    "babel-node": "babel-node",
    "start": "npm run babel-node -- server/server.js",
    "startd": "nodemon --exec npm run babel-node -- server/server.js",
    "test": "mocha --require babel-register",
    "test-watch": "export WATCH='watch' && nodemon --exec 'npm test'",
    "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules",
    "serve": "node dist/server.js",
    "start_babel": "babel-node server/server.js --preset=babel-preset-es2015",
    "deploy2test": "bash deploy.sh",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "Express"
  ],
  "author": "klequis",
  "dependencies": {
    "aws-sdk": "^2.256.1",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "crypto": "1.0.1",
    "dotenv": "6.0.0",
    "express": "^4.16.4",
    "express-jwt": "5.3.1",
    "express-session": "1.15.6",
    "formidable": "^1.2.1",
    "jsonwebtoken": "8.3.0",
    "mongodb": "^3.1.3",
    "node-fetch": "^3.1.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pify": "^4.0.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "^0.0.41",
    "@types/express-session": "^1.15.11",
    "@types/formidable": "^1.0.31",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.19",
    "@types/morgan": "^1.7.35",
    "@types/node-fetch": "^2.1.4",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/pify": "^3.0.2",
    "@types/ramda": "^0.25.47",
    "@types/supertest": "^2.0.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "chalk": "2.4.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "mocha": "^5.1.1",
    "morgan": "^1.9.1",
    "nodemon": "1.17.1",
    "supertest": "3.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
